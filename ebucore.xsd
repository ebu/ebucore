	<complexType name="timeType">
		<choice>
			<element name="timecode" type="ebucore:timecodeType">
				<annotation>
					<documentation> To express time as timecode. </documentation>
				</annotation>
			</element>
			<element name="normalPlayTime" type="time">
				<annotation>
					<documentation>To express the start time in the form HH:MM:SS.S</documentation>
				</annotation>
			</element>
			<element name="offsetNormalPlayTime" type="duration">
				<annotation>
					<documentation>To express a time offset as a duration in the iso duration format PnYnMnDTnHnMnS e.g. PT1M40.1S</documentation>
				</annotation>
			</element>
			<element name="editUnitNumber" type="ebucore:editUnitNumberType">
				<annotation>
