				</annotation>
				<complexType>
					<attributeGroup ref="ebucore:dateGroup">
						<annotation>
							<documentation> A set of attributes to express a date or a date period.
							</documentation>
						</annotation>
					</attributeGroup>
				</complexType>
			</element>
			<element name="alternative" type="ebucore:alternativeDateType" minOccurs="0"
				maxOccurs="unbounded">
				<annotation>
					<documentation>An alternative particular date and optionally time for which the
						type can be defined.</documentation>
				</annotation>
			</element>
			<element name="ingested" minOccurs="0">
				<annotation>
					<documentation>The date and optionally time when the resource was
						ingested.</documentation>
				</annotation>
				<complexType>
					<attributeGroup ref="ebucore:dateGroup">
						<annotation>
							<documentation> A set of attributes to express a date or a date period.
							</documentation>
						</annotation>
					</attributeGroup>
				</complexType>
			</element>
			<element name="archived" minOccurs="0">
				<annotation>
					<documentation>The date and optionally time when the resource has been
						archived.</documentation>
				</annotation>
				<complexType>
					<attributeGroup ref="ebucore:dateGroup">
						<annotation>
							<documentation> A set of attributes to express a date or a date period.
							</documentation>
						</annotation>
					</attributeGroup>
				</complexType>
			</element>
			<element name="note" type="string" minOccurs="0"/>
		</sequence>
		<attributeGroup ref="ebucore:typeGroup">
			<annotation>
				<documentation> to define a type of date associated with dc:date </documentation>
			</annotation>
		</attributeGroup>
		<attributeGroup ref="ebucore:formatGroup">
			<annotation>
				<documentation> to define the format of date used in dc:date</documentation>
			</annotation>
		</attributeGroup>
		<attribute name="precision" type="string">
