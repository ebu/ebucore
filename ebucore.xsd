	<complexType name="contactDetailsType">
		<annotation>
			<documentation> To provide all the information necessary to contact and locate a person.
			</documentation>
		</annotation>
		<sequence>
			<choice>
				<element name="name" type="ebucore:compoundNameType" minOccurs="0"
					maxOccurs="unbounded">
					<annotation>
						<documentation>The contact name is optional as a contact maybe identified by
							its contactId. The name is a combination of different elements of
							information for a specific purpose such as a name for
							display.</documentation>
					</annotation>
				</element>
				<sequence>
					<element name="givenName" type="ebucore:elementType" minOccurs="0">
						<annotation>
							<documentation>Also known as the first name among
								others.</documentation>
						</annotation>
					</element>
					<element name="familyName" type="ebucore:elementType" minOccurs="0">
						<annotation>
							<documentation> also known as the last name among
								others.</documentation>
						</annotation>
					</element>
					<element name="otherGivenName" type="ebucore:elementType" minOccurs="0"
						maxOccurs="unbounded">
						<annotation>
							<documentation>e.g. a middle name.</documentation>
						</annotation>
					</element>
					<element name="suffix" type="ebucore:elementType" minOccurs="0">
						<annotation>
							<documentation> E.g. 'Jr' for junior or 'Sr' for senior.
							</documentation>
						</annotation>
					</element>
					<element name="salutation" type="ebucore:elementType" minOccurs="0">
						<annotation>
							<documentation>E.g. M., Ms., Mrs, Pr., Dr.</documentation>
						</annotation>
					</element>
				</sequence>
			</choice>
			<element name="birthDate" type="date" minOccurs="0">
				<annotation>
					<documentation> The date of birth. </documentation>
				</annotation>
			</element>
			<element name="deathDate" type="date" minOccurs="0">
				<annotation>
					<documentation> The date of death. If absent, means that the contact is still
						alive.</documentation>
				</annotation>
			</element>
			<element name="birthPlace" type="string" minOccurs="0">
				<annotation>
					<documentation> The place of birth. </documentation>
				</annotation>
			</element>
			<element name="deathPlace" type="string" minOccurs="0">
				<annotation>
					<documentation> The place of death. </documentation>
				</annotation>
			</element>
			<element name="nationality" type="string" minOccurs="0">
				<annotation>
					<documentation> The current nationality. </documentation>
				</annotation>
			</element>
			<element name="username" type="ebucore:elementType" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<documentation>The name used by the contact in a particular
						context.</documentation>
				</annotation>
			</element>
			<element name="nickname" type="ebucore:elementType" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<documentation>A name given to the contact.</documentation>
				</annotation>
			</element>
			<element name="occupation" type="ebucore:elementType" minOccurs="0">
				<annotation>
					<documentation>The job function of the contact</documentation>
				</annotation>
			</element>
			<element name="details" type="ebucore:detailsType" minOccurs="0" maxOccurs="unbounded">
				<annotation>
