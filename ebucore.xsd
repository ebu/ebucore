	<complexType name="documentFormatType">
		<annotation>
			<documentation> To list the technical characteristics of a document. </documentation>
		</annotation>
		<sequence>
			<element name="wordCount" type="integer" minOccurs="0">
				<annotation>
					<documentation>To provide a word count for the document</documentation>
				</annotation>
			</element>
			<element name="regionDelimX" type="ebucore:dimensionType" minOccurs="0">
				<annotation>
					<documentation>The identification of a region in a document, an image or a video
						is done by defining the coordinates of the bottom left corner of the region.
						The region is defined from this point of reference using the width and
						height properties. regionDelimX uses the same unit as width.</documentation>
				</annotation>
			</element>
			<element name="regionDelimY" type="ebucore:dimensionType" minOccurs="0">
				<annotation>
					<documentation>The identification of a region in a document, an image or a video
						is done by defining the coordinates of the bottom left corner of the region.
						The region is defined from this point of reference using the width and
						height properties. regionDelimY uses the same unit as
						height.</documentation>
				</annotation>
			</element>
			<element name="width" type="ebucore:dimensionType" minOccurs="0">
				<annotation>
					<documentation>The width of an image, video or document.</documentation>
				</annotation>
			</element>
			<element name="height" type="ebucore:dimensionType" minOccurs="0">
				<annotation>
					<documentation>The height of an image, video or document.</documentation>
				</annotation>
			</element>
			<group ref="ebucore:technicalAttributes">
				<annotation>
					<documentation> A point of extension for customisation using technical
						attributes of predefined datatypes. </documentation>
				</annotation>
			</group>
			<element ref="ebucore:comment" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<documentation> To provide additional contextual information. </documentation>
				</annotation>
			</element>
