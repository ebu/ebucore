	<complexType name="digitalAssetColorVolumeType">
		<annotation>
			<documentation>HDR (High Dynamic Range) static metadata. Naming similar to MovieLabs'
				common metadata set and SMPTE 2086.</documentation>
		</annotation>
		<sequence>
			<element name="primaryRChromaticity" type="ebucore:digitalAssetChromaticityType"/>
			<element name="primaryGChromaticity" type="ebucore:digitalAssetChromaticityType"/>
			<element name="primaryBChromaticity" type="ebucore:digitalAssetChromaticityType"/>
			<element name="whitePointChromaticity" type="ebucore:digitalAssetChromaticityType"/>
			<element name="luminanceMin">
				<annotation>
					<documentation> Minimum luminance is expressed in cd/m2. </documentation>
				</annotation>
				<simpleType>
					<restriction base="decimal">
						<totalDigits value="5"/>
						<fractionDigits value="4"/>
						<maxInclusive value="5.0000"/>
						<minInclusive value="0.0001"/>
					</restriction>
				</simpleType>
			</element>
			<element name="luminanceMax">
				<annotation>
					<documentation> Maximum luminance is expressed in cd/m2. </documentation>
				</annotation>
				<simpleType>
					<restriction base="decimal">
						<totalDigits value="7"/>
						<fractionDigits value="2"/>
						<maxInclusive value="10000.00"/>
						<minInclusive value="5.00"/>
					</restriction>
				</simpleType>
			</element>
		</sequence>
	</complexType>

	<complexType name="digitalAssetChromaticityType">
		<sequence>
			<element name="ChromaticityCIEx">
				<simpleType>
					<restriction base="decimal">
						<totalDigits value="5"/>
						<fractionDigits value="4"/>
						<maxInclusive value="0.7400"/>
						<minInclusive value="0.0001"/>
					</restriction>
				</simpleType>
			</element>
			<element name="ChromaticityCIEy">
				<simpleType>
					<restriction base="decimal">
						<totalDigits value="5"/>
						<fractionDigits value="4"/>
						<maxInclusive value="0.7400"/>
						<minInclusive value="0.0001"/>
					</restriction>
				</simpleType>
			</element>
		</sequence>
	</complexType>

