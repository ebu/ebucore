					<simpleContent>
						<extension base="string">
							<attributeGroup ref="ebucore:typeGroup"/>
						</extension>
					</simpleContent>
				</complexType>
			</element>
			<element name="artefacttype" minOccurs="0">
				<complexType>
					<simpleContent>
						<extension base="string">
							<attributeGroup ref="ebucore:typeGroup"/>
						</extension>
					</simpleContent>
				</complexType>
			</element>
			<element name="artefactColour" type="string" minOccurs="0"/>
			<element name="artefactWeight" type="string" minOccurs="0"/>
			<element name="artefactReference" type="string" minOccurs="0"/>
			<element name="artefactPrice" minOccurs="0" maxOccurs="unbounded">
				<complexType>
					<sequence>
						<element name="value" type="float"/>
						<element name="currency" minOccurs="0">
							<complexType>
								<attributeGroup ref="ebucore:typeGroup"/>
							</complexType>
						</element>
					</sequence>
				</complexType>
			</element>
			<element name="artefactWebsite" type="anyURI" minOccurs="0"/>
			<element name="artefactDateOfPurchase" type="date" minOccurs="0"/>
			<element name="artefactDateOfSell" type="date" minOccurs="0"/>
			<element name="artefactDateOfCreation" type="date" minOccurs="0"/>
			<element name="artefactDateOfAcquisition" type="date" minOccurs="0"/>
			<element name="artefactDateOfDestruction" type="date" minOccurs="0"/>
			<element name="artefactAvailability" type="boolean" minOccurs="0"/>
			<element name="artefactLocation" type="string" minOccurs="0"/>
			<element name="artefactUsageHistory" type="ebucore:elementType" minOccurs="0"
				maxOccurs="unbounded"/>
			<element name="artefactStyle" type="ebucore:elementType" minOccurs="0"/>
			<element name="artefactPeriod" minOccurs="0">
				<complexType>
					<attributeGroup ref="ebucore:dateGroup"/>
				</complexType>
			</element>
			<element name="relatedAgent" type="ebucore:entityType" minOccurs="0"
				maxOccurs="unbounded"/>
			<element name="supplier" type="ebucore:entityType" minOccurs="0" maxOccurs="unbounded"/>
			<element name="retailer" type="ebucore:entityType" minOccurs="0" maxOccurs="unbounded"/>
			<element name="buyer" type="ebucore:entityType" minOccurs="0" maxOccurs="unbounded"/>
			<element name="creator" type="ebucore:entityType" minOccurs="0" maxOccurs="unbounded"/>
			<element name="maker" type="ebucore:entityType" minOccurs="0" maxOccurs="unbounded"/>
			<element name="owner" type="ebucore:entityType" minOccurs="0" maxOccurs="unbounded"/>
			<element name="contact" type="ebucore:entityType" minOccurs="0" maxOccurs="unbounded"/>
			<element name="timestamp" type="ebucore:timeType" minOccurs="0">
				<annotation>
					<documentation> A timestamp to precisely define a time point when the artefact
						is used.. </documentation>
				</annotation>
			</element>
			<element name="additionalInformation" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<documentation>To provide additional custom information about an
						artefact.</documentation>
				</annotation>
				<complexType>
					<simpleContent>
						<extension base="ebucore:elementType">
							<attributeGroup ref="ebucore:typeGroup"/>
						</extension>
					</simpleContent>
				</complexType>
			</element>
		</sequence>
		<attribute name="artefactid" type="string">
			<annotation>
				<documentation>The id attributed to an artefact.</documentation>
			</annotation>
		</attribute>
		<attributeGroup ref="ebucore:typeGroup"/>
	</complexType>
	<complexType name="propsType">
