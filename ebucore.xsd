	<!-- GENERAL TYPES -->
	<simpleType name="timecodeStringType">
		<annotation>
			<documentation>The pattern to express the time using timecode compliant with SMPTE
				ST12-1:2008</documentation>
		</annotation>
		<restriction base="string">
			<pattern value="(([0-1][0-9])|([2][0-3])):[0-5][0-9]:[0-5][0-9](([.,])|([:;]))[0-9]{2,}"
			/>
		</restriction>
	</simpleType>
	<simpleType name="timecodeStringFrameType">
		<annotation>
			<documentation>The pattern to express the time using timecode compliant with SMPTE
				ST12-1:2008</documentation>
		</annotation>
		<restriction base="string">
			<pattern
				value="([0-1][0-9]|[2][0-3]):[0-5][0-9]:[0-5][0-9]([:;][0-5][0-9]|[:;][0-5][0-9][.,][0-9]{5,})"
			/>
		</restriction>
	</simpleType>

	<complexType name="timecodeType">
		<simpleContent>
			<extension base="ebucore:timecodeStringType">
				<attribute name="editRate" type="integer"/>
				<attribute name="dropframe" type="boolean"/>
				<attribute name="factorNumerator" type="positiveInteger" default="1">
					<annotation>
						<documentation> The numerator of a correcting factor. </documentation>
					</annotation>
				</attribute>
				<attribute name="factorDenominator" type="positiveInteger" default="1">
					<annotation>
						<documentation> The denominator of a correcting factor. </documentation>
					</annotation>
				</attribute>
			</extension>
		</simpleContent>
	</complexType>
	<complexType name="timecodeFrameType">
		<simpleContent>
			<extension base="ebucore:timecodeStringFrameType">
				<attribute name="editRate" type="integer"/>
				<attribute name="dropframe" type="boolean"/>
				<attribute name="factorNumerator" type="positiveInteger" default="1">
